    "express-validator": "^6.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15"
  }
